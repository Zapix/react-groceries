  "scripts": {
    "build": "npm run clean && npm run lint && webpack --display-error-details",
    "build:i18n": "rimraf _translations && babel --quiet --babelrc .babelrc.i18n lib > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node scripts/i18nToXliff.js",
    "clean": "rimraf dist",
    "deploy": "npm run dist && gh-pages -d dist",
    "dist": "npm run clean && npm run lint && NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint lib --ext=js --ext=jsx",
    "start": "npm run build && node server.js",
    "start:devtools": "npm run build && DEVTOOLS=true node server.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "coverage": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'"
  },
